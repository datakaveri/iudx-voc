            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:RoadSegment"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:NumericArray"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ]
        }
    ]
