                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:Number"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "GTFS:route_type"
            }
        }
