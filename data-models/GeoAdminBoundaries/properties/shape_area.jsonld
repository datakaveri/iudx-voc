{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/"
    },
    "@graph": [
        {
            "@id": "iudx:shape_area",
            "@type": [
                "iudx:QuantitativeProperty"
            ],
            "rdfs:comment": "The total area enclosed within the polygon or by the boundary lining the shape of entity corresponding to this observation.",
            "rdfs:label": "shape_area",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:District"
                },
                {
                    "@id": "iudx:States"
                },
                {
                    "@id": "iudx:FeaturesOfInterest"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Number"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        }
    ]
}