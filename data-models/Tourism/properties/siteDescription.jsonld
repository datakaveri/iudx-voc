{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/"
    },
    "@graph": [
        {
            "@id": "iudx:siteDescription",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "The detailed description about a public site or tourist destination corresponding to this observation.",
            "rdfs:label": "siteDescription",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:TouristDestination"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ]
        }
    ]
}