{
    "@graph": [
        {
            "@id": "iudx:StopsDesc",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "A Data Model for a vehicle route's stop information.",
            "rdfs:label": "StopsDesc",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:stopInfo",
            "@type": [
                "iudx:StructuredProperty"
            ],
            "rdfs:comment": "Stop related information corresponding to this observation.",
            "rdfs:label": "stopInfo",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:TransitManagement"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:StopsDesc"
                }
            ]
        },
        {
            "@id": "iudx:stop_code",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "This field contains short text or a number that uniquely identifies the stop for passengers. Can be same as stop_id if it is for public. SameAs: 'stop_code' field from GTFS Static Field definitions-stops.txt (https://developers.google.com/transit/gtfs/reference#stopstxt)",
            "rdfs:label": "stop_code",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:StopsDesc"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "GTFS: stop_code"
            }
        },
        {
            "@id": "iudx:stop_desc",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "This field contains a description of a stop. SameAs: 'stop_desc' field from GTFS Static Field definitions-stops.txt (https://developers.google.com/transit/gtfs/reference#stopstxt)",
            "rdfs:label": "stop_desc",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:StopsDesc"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "GTFS: stop_desc"
            }
        },
        {
            "@id": "iudx:stop_id",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Unique ID assigned to the stop corresponding to this observation.",
            "rdfs:label": "stop_id",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:GtfsCommons"
                },
                {
                    "@id": "iudx:VehiclePosition"
                },
                {
                    "@id": "iudx:StopTimeUpdate"
                },
                {
                    "@id": "iudx:StopsDesc"
                },
                {
                    "@id": "iudx:StopTimesDesc"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "GTFS: stop_id"
            }
        },
        {
            "@id": "iudx:stop_name",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Describes the name of a stop or station. SameAs: 'stop_name' field from GTFS Static Field definitions-stops.txt (https://developers.google.com/transit/gtfs/reference#stopstxt)",
            "rdfs:label": "stop_name",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:StopsDesc"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "GTFS: stop_name"
            }
        },
        {
            "@id": "iudx:stop_url",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "This field contains the URL of a web page about a particular stop and is different from the agency_url and the route_url fields. SameAs: 'stop_url' field from GTFS Static Field definitions-stops.txt (https://developers.google.com/transit/gtfs/reference#stopstxt)",
            "rdfs:label": "stop_url",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:StopsDesc"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "GTFS: stop_url"
            }
        }
    ],
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "iudx": "https://voc.iudx.org.in/",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "schema": "http://schema.org/",
        "geojson": "https://purl.org/geojson/vocab#",
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "xsd": "http://www.w3.org/2001/XMLSchema#"
    }
}