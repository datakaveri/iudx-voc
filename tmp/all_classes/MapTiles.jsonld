{
    "@graph": [
        {
            "@id": "iudx:GeoSpatialInfo",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Data Models for Geospatial Information.",
            "rdfs:label": "GeoSpatialInfo",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:DataModel"
            }
        },
        {
            "@id": "iudx:DataModel",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Class of all Data Models.",
            "rdfs:label": "DataModel",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:Thing"
            }
        },
        {
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "@id": "iudx:MapTiles",
            "rdfs:comment": "A Data Model for Map Tiles.",
            "rdfs:label": "MapTiles",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:GeoSpatialInfo"
            }
        },
        {
            "@id": "iudx:Thing",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Most generic IUDX object.",
            "rdfs:label": "Thing",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "skos:exactMatch": {
                "@id": "schema:Thing"
            }
        },
        {
            "@id": "iudx:dataDescriptor",
            "@type": [
                "iudx:Relationship"
            ],
            "rdfs:comment": "URI pointing to the data-descriptor entity",
            "rdfs:label": "dataDescriptor",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:ResourceGroup"
                },
                {
                    "@id": "iudx:Resource"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@type": "@id",
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:RelationshipValue"
                },
                {
                    "@id": "iudx:DataModel"
                }
            ]
        },
        {
            "@id": "iudx:description",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Description for an IUDX thing.",
            "rdfs:label": "description",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Thing"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                }
            ],
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:imageBounds",
            "@type": [
                "iudx:GeoProperty"
            ],
            "rdfs:comment": "A bbox in the form [lowerlon, lowerlat, upperlon, upperlat].",
            "rdfs:label": "imageBounds",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:MapTiles"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:GeoJSONGeometry"
                },
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ]
        },
        {
            "@id": "iudx:imageFormat",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "The format of the image corresponding this observation. It can take up values like png, jpg, etc",
            "rdfs:label": "imageFormat",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:MapTiles"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ]
        },
        {
            "@id": "iudx:maxZoom",
            "@type": [
                "iudx:QuantitativeProperty"
            ],
            "rdfs:comment": "A number between 0 to 18 denoting maximum zoom.",
            "rdfs:label": "maxZoom",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:MapTiles"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Number"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ]
        },
        {
            "@id": "iudx:minZoom",
            "@type": [
                "iudx:QuantitativeProperty"
            ],
            "rdfs:comment": "A number between 0 to 18 denoting minimum zoom.",
            "rdfs:label": "minZoom",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:MapTiles"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Number"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ]
        }
    ],
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/",
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "geojson": "https://purl.org/geojson/vocab#",
        "schema": "http://schema.org/"
    }
}