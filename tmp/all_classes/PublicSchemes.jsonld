{
    "@graph": [
        {
            "@id": "iudx:Civic",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Data models that pertain to the citizen issues/civic domain",
            "rdfs:label": "Civic",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:DataModel"
            }
        },
        {
            "@id": "iudx:DataModel",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Class of all Data Models.",
            "rdfs:label": "DataModel",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:Thing"
            }
        },
        {
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "@id": "iudx:PublicSchemes",
            "rdfs:comment": "A Data Model for public welfare schemes",
            "rdfs:label": "PublicSchemes",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:Civic"
            }
        },
        {
            "@id": "iudx:Thing",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Most generic IUDX object.",
            "rdfs:label": "Thing",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "skos:exactMatch": {
                "@id": "schema:Thing"
            }
        },
        {
            "@id": "iudx:beneficiaryCount",
            "@type": [
                "iudx:QuantitativeProperty"
            ],
            "rdfs:comment": "Represents the count of people benifited under the scheme corresponding to this observation.",
            "rdfs:label": "beneficiaryCount",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:PublicSchemes"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Number"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ]
        },
        {
            "@id": "iudx:dataDescriptor",
            "@type": [
                "iudx:Relationship"
            ],
            "rdfs:comment": "URI pointing to the data-descriptor entity",
            "rdfs:label": "dataDescriptor",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:ResourceGroup"
                },
                {
                    "@id": "iudx:Resource"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@type": "@id",
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:RelationshipValue"
                },
                {
                    "@id": "iudx:DataModel"
                }
            ]
        },
        {
            "@id": "iudx:department",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Division or section of the organization corresponding to this observation. Eg. Health, Transport.",
            "rdfs:label": "department",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:EmployeeDetails"
                },
                {
                    "@id": "iudx:IssueReporting"
                },
                {
                    "@id": "iudx:EnergyMeter"
                },
                {
                    "@id": "iudx:PublicSchemes"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "skos:exactMatch": {
                "@id": "schema:department"
            }
        },
        {
            "@id": "iudx:departmentID",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Unique ID or code associated with the department which is associated with the service or the issue corresponding to this observation.",
            "rdfs:label": "departmentID",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:IssueReporting"
                },
                {
                    "@id": "iudx:EmployeeDetails"
                },
                {
                    "@id": "iudx:PublicSchemes"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:description",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Description for an IUDX thing.",
            "rdfs:label": "description",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Thing"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                }
            ],
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:eligibleBeneficiaries",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Represents the group(s) or category(ies) of people who can avail the benefits of the scheme corresponding to this observation.",
            "rdfs:label": "eligibleBeneficiaries",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:PublicSchemes"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@container": "@list",
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ]
        },
        {
            "@id": "iudx:grantAmount",
            "@type": [
                "iudx:QuantitativeProperty"
            ],
            "rdfs:comment": "The amount released for the public welfare under the scheme from the total budget corresponding to this observation.",
            "rdfs:label": "grantAmount",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:PublicSchemes"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Number"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema: funding"
            }
        },
        {
            "@id": "iudx:sanctionedAmount",
            "@type": [
                "iudx:QuantitativeProperty"
            ],
            "rdfs:comment": "The actual amount spent from the grant/allocated amount for the scheme.",
            "rdfs:label": "sanctionedAmount",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:PublicSchemes"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Number"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ]
        },
        {
            "@id": "iudx:schemeID",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "The unique ID representing the scheme corresponding to this observation.",
            "rdfs:label": "schemeID",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:PublicSchemes"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ]
        },
        {
            "@id": "iudx:schemeName",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "The name of the scheme or programme introduced by the government for public welfare and development.",
            "rdfs:label": "schemeName",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:PublicSchemes"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ]
        },
        {
            "@id": "iudx:year",
            "@type": [
                "iudx:QuantitativeProperty"
            ],
            "rdfs:comment": "Year corresponding to this observation and is described in YYYY format, for eg. 2020.",
            "rdfs:label": "year",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:RevenueCollection"
                },
                {
                    "@id": "iudx:PublicSchemes"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Number"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        }
    ],
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://voc.iudx.org.in/",
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "geojson": "https://purl.org/geojson/vocab#",
        "schema": "http://schema.org/"
    }
}