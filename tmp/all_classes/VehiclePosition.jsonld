{
    "@graph": [
        {
            "@id": "iudx:VehiclePosition",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "A Data Model for vehicle's realtime positioning.",
            "rdfs:label": "VehiclePosition",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:congestion_level",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Describes the congestion level that is affecting this vehicle. ENUM [UNKNOWN_CONGESTION_LEVEL, RUNNING_SMOOTHLY, STOP_AND_GO, CONGESTION, SEVERE_CONGESTION]",
            "rdfs:label": "congestion_level",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:VehiclePosition"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "GTFS: congestion_level"
            }
        },
        {
            "@id": "iudx:current_status",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Describes the status of the vehicle w.r.t the stop corresponding to this observation ENUM: [INCOMING_AT, STOPPED_AT, IN_TRANSIT_TO]. SameAs:'current_status' field from GTFS Realtime message-VehiclePosition(https://developers.google.com/transit/gtfs-realtime/reference#message-vehicleposition)",
            "rdfs:label": "current_status",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:VehiclePosition"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "GTFS: current_status"
            }
        },
        {
            "@id": "iudx:current_stop_sequence",
            "@type": [
                "iudx:QuantitativeProperty"
            ],
            "rdfs:comment": "Gives the stop sequence index of the current stop. This is determined by considering current_status, if current_status is missing IN_TRANSIT_TO is assumed. SameAs:'current_stop_sequence' field from GTFS Realtime message-VehiclePosition(https://developers.google.com/transit/gtfs-realtime/reference#message-vehicleposition)",
            "rdfs:label": "current_stop_sequence",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:VehiclePosition"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Number"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "GTFS: current_stop_sequence"
            }
        },
        {
            "@id": "iudx:occupancy_status",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "The degree of passenger occupancy for the vehicle. ENUM [EMPTY,  MANY_SEATS_AVAILABLE, FEW_SEATS_AVAILABLE, STANDING_ROOM_ONLY, CRUSHED_STANDING_ROOM_ONLY, FULL,  NOT_ACCEPTING_PASSENGERS, NO_DATA_AVAILABLE, NOT_BOARDABLE]",
            "rdfs:label": "occupancy_status",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:VehiclePosition"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "GTFS: occupancy_status"
            }
        },
        {
            "@id": "iudx:stop_id",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Unique ID assigned to the stop corresponding to this observation.",
            "rdfs:label": "stop_id",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:GtfsCommons"
                },
                {
                    "@id": "iudx:VehiclePosition"
                },
                {
                    "@id": "iudx:StopTimeUpdate"
                },
                {
                    "@id": "iudx:StopsDesc"
                },
                {
                    "@id": "iudx:StopTimesDesc"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "GTFS: stop_id"
            }
        },
        {
            "@id": "iudx:vehiclePositionInfo",
            "@type": [
                "iudx:StructuredProperty"
            ],
            "rdfs:comment": "Describes the realtime position of the vehicle corresponding to this observation. SameAs:'vehicle' field from GTFS Realtime message-FeedEntity(https://developers.google.com/transit/gtfs-realtime/reference#message-feedentity)",
            "rdfs:label": "vehiclePositionInfo",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:TransitManagement"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:VehiclePosition"
                }
            ]
        }
    ],
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "iudx": "https://voc.iudx.org.in/",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "schema": "http://schema.org/",
        "geojson": "https://purl.org/geojson/vocab#",
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "xsd": "http://www.w3.org/2001/XMLSchema#"
    }
}