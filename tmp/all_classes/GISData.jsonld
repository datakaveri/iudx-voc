                {
                    "@id": "iudx:ResourceGroup"
                },
                {
                    "@id": "iudx:Resource"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@type": "@id",
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:RelationshipValue"
                },
                {
                    "@id": "iudx:DataModel"
                }
            ]
        },
        {
            "@id": "iudx:description",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Description for an IUDX thing.",
