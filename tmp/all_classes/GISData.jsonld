            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                }
            ],
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:gisAccessToken",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "A string of encrypted information that contains the user's name, the token expiration time, and some proprietary information, which is used to access the secured GIS services from the GIS Server.",
            "rdfs:label": "gisAccessToken",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:GISData"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ]
        },
        {
            "@id": "iudx:gisLayers",
            "@type": [
                "iudx:TextProperty"
            ],
