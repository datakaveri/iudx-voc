{
    "@graph": [
        {
            "@id": "iudx:Civic",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Data models that pertain to the citizen issues/civic domain",
            "rdfs:label": "Civic",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:DataModel"
            }
        },
        {
            "@id": "iudx:DataModel",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Class of all Data Models.",
            "rdfs:label": "DataModel",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:Thing"
            }
        },
        {
            "@id": "iudx:GISData",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "A Data Model Geographic Information System data.",
            "rdfs:label": "GISData",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:Civic"
            }
        },
        {
