{
    "@graph": [
        {
            "@id": "iudx:TripDescriptor",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "A Data Model for vehicle trip's description.",
            "rdfs:label": "TripDescriptor",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:direction_id",
            "@type": [
                "iudx:QuantitativeProperty"
            ],
            "rdfs:comment": "Indicates the direction of travel of the vehicle corresponding to this observation, can be referenced from the GTFS static feed trips.txt. SameAs: 'direction_id' field from GTFS Realtime message-TripDescriptor (https://developers.google.com/transit/gtfs-realtime/reference#message-tripdescriptor)",
            "rdfs:label": "direction_id",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:TripDescriptor"
                },
                {
                    "@id": "iudx:FareCollectionSystem"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Number"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ]
        },
        {
            "@id": "iudx:route_id",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Route ID assigned to the route on which the bus/vehicle corresponding to the bus in this observation is currently plying on.",
            "rdfs:label": "route_id",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:GtfsCommons"
                },
                {
                    "@id": "iudx:TripDescriptor"
                },
                {
                    "@id": "iudx:FareCollectionSystem"
                },
                {
                    "@id": "iudx:RoutesDesc"
                },
                {
                    "@id": "iudx:TransitManagement"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "skos:exactMatch": {
                "@id": "GTFS: route_id"
            }
        },
        {
            "@id": "iudx:schedule_relationship",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Describes if the Route/Trip has been scheduled. SameAs: 'schedule_relationship' field from GTFS Realtime message-TripDescriptor ENUM[SCHEDULED, ADDED, UNSCHEDULED, CANCELED] (https://developers.google.com/transit/gtfs-realtime/reference#message-tripdescriptor)",
            "rdfs:label": "schedule_relationship",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:TripDescriptor"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "GTFS: schedule_relationship"
            }
        },
        {
            "@id": "iudx:start_date",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Describes the initial scheduled date of the trip corresponding to the vehicle this observation. An example format for this field - YYYYMMDD. SameAs: 'start_date' field from GTFS Realtime message-TripDescriptor (https://developers.google.com/transit/gtfs-realtime/reference#message-tripdescriptor)",
            "rdfs:label": "start_date",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:TripDescriptor"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "GTFS: start_date"
            }
        },
        {
            "@id": "iudx:start_time",
            "@type": [
                "iudx:TimeProperty"
            ],
            "rdfs:comment": "Describes the initial scheduled start time of the trip corresponding to the vehicle this observation. An example format for this field - 11:15:35 or 25:15:35. SameAs: 'start_time' field from GTFS Realtime message-TripDescriptor (https://developers.google.com/transit/gtfs-realtime/reference#message-tripdescriptor)",
            "rdfs:label": "start_time",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:TripDescriptor"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "GTFS: start_time"
            }
        },
        {
            "@id": "iudx:tripInfo",
            "@type": [
                "iudx:StructuredProperty"
            ],
            "rdfs:comment": "Describes the trip the vehicle corresponding to this observation is making. SameAs:'trip' field from GTFS Realtime message-VehiclePosition(https://developers.google.com/transit/gtfs-realtime/reference#message-vehicleposition)(https://developers.google.com/transit/gtfs-realtime/reference#message-tripupdate)",
            "rdfs:label": "tripInfo",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:TransitManagement"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:TripDescriptor"
                }
            ]
        },
        {
            "@id": "iudx:trip_id",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Trip ID/Trip name allotted to the bus corresponding to this observation, in consideration to the time of the day and the direction of the trip on the given routeId.",
            "rdfs:label": "trip_id",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:GtfsCommons"
                },
                {
                    "@id": "iudx:TripDescriptor"
                },
                {
                    "@id": "iudx:FareCollectionSystem"
                },
                {
                    "@id": "iudx:StopTimesDesc"
                },
                {
                    "@id": "iudx:WmgmtVehicle"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "skos:exactMatch": {
                "@id": "GTFS: trip_id"
            }
        }
    ],
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "iudx": "https://voc.iudx.org.in/",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "schema": "http://schema.org/",
        "geojson": "https://purl.org/geojson/vocab#",
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "xsd": "http://www.w3.org/2001/XMLSchema#"
    }
}