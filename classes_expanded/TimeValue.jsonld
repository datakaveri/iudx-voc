{
    "@context": {
        "iudx": "https://iudx.org.in/voc/",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#"
    },
    "@graph": [
        {
            "@id": "iudx:TimeValue",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:label": "TimeValue",
            "rdfs:comment": "Describes the value of a Temporal attribute",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:hasValue",
            "@type": [
                "owl:DatatypeProperty",
                "rdf:Property"
            ],
            "rdfs:comment": {
                "@value": "Value associated with PropertyValue, QuantitativeValue and RelationshipValue."
            },
            "rdfs:label": {
                "@value": "Value of a property or attribute"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:PropertyValue"
                },
                {
                    "@id": "iudx:QuantitativeValue"
                },
                {
                    "@id": "iudx:GeoValue"
                },
                {
                    "@id": "iudx:TimeValue"
                },
                {
                    "@id": "iudx:RelationshipValue"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:string",
                    "@type": "@id"
                }
            ]
        }
    ]
}