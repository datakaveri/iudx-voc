{
    "@context": {
        "iudx": "https://iudx.org.in/voc/",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#"
    },
    "@graph": [
        {
            "@id": "iudx:PropertyValue",
            "@type": "rdfs:Class",
            "owl:equivalentClass": {
                "@id": "rdf:Property"
            },
            "rdfs:comment": "A property, used to indicate attributes and relationships of some Thing; equivalent to rdf:Property.",
            "rdfs:label": "Property",
            "rdfs:subClassOf": {
                "@id": "iudx:Intangible"
            },
            "iudx:isPartOf": {
                "@id": "https://iudx.org.in/voc/meta"
            }
        },
        {
            "@id": "iudx:hasValue",
            "@type": [
                "owl:DatatypeProperty",
                "rdf:Property"
            ],
            "rdfs:comment": {
                "@value": "Value associated with PropertyValue, QuantitativeValue and RelationshipValue."
            },
            "rdfs:label": {
                "@value": "Value of a property or attribute"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:PropertyValue"
                },
                {
                    "@id": "iudx:QuantitativeValue"
                },
                {
                    "@id": "iudx:GeoValue"
                },
                {
                    "@id": "iudx:TimeValue"
                },
                {
                    "@id": "iudx:RelationshipValue"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:string",
                    "@type": "@id"
                }
            ]
        },
        {
            "@id": "iudx:name",
            "@type": [
                "rdf:Property",
                "iudx:Property"
            ],
            "owl:equivalentProperty": {
                "@id": "dcterms:title"
            },
            "rdfs:comment": "The name of the property",
            "rdfs:label": "name",
            "rdfs:subPropertyOf": {
                "@id": "rdfs:label"
            },
            "iudx:domainIncludes": {
                "@id": "iudx:PropertyValue"
            },
            "iudx:rangeIncludes": {
                "@id": "iudx:Text"
            }
        }
    ]
}