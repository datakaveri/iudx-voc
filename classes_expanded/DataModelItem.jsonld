{
    "@context": {
        "iudx": "https://iudx.org.in/voc/",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#"
    },
    "@graph": [
        {
            "@id": "iudx:DataModelItem",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:label": "Data Model Item",
            "rdfs:comment": "Describes the Data Model Item entry in the IUDX catalogue.",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:itemDescription",
            "@type": [
                "rdf:Property",
                "iudx:TextProperty"
            ],
            "rdfs:comment": {
                "@value": "Description for the catalouge item"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": {
                "@value": "Description"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:ResourceItem"
                },
                {
                    "@id": "iudx:ResourceGroupItem"
                },
                {
                    "@id": "iudx:ResourceServerItem"
                },
                {
                    "@id": "iudx:ProviderItem"
                },
                {
                    "@id": "iudx:DataModelItem"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:string"
                }
            ]
        }
    ]
}