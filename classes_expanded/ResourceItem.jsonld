{
    "@context": {
        "iudx": "https://iudx.org.in/voc/",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#"
    },
    "@graph": [
        {
            "@id": "iudx:ResourceItem",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Describes the Resource Item entry in the IUDX catalogue.",
            "rdfs:label": "Resource Item",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:hasResourceGroup",
            "@type": [
                "rdf:Property",
                "iudx:Relationship"
            ],
            "rdfs:comment": "urn pointing to the resourceGroup entity",
            "rdfs:label": "hasResourceGroup",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:ResourceItem"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:string",
                    "@type": "@id"
                },
                {
                    "@id": "iudx:RelationshipValue"
                }
            ]
        }
    ]
}