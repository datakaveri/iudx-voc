{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "schema": "http://schema.org/",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://iudx.org.in/voc/",
        "geojson": "https://purl.org/geojson/vocab#",
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "xsd": "http://www.w3.org/2001/XMLSchema#"
    },
    "@graph": [
        {
            "@id": "iudx:unitCode",
            "@type": [
                "rdf:Property",
                "iudx:TextProperty"
            ],
            "rdfs:comment": "The unit of measurement given using the UN/CEFACT Common Code (3 characters) or a URL. Other codes/unit vocabularies may be used with a prefix",
            "rdfs:label": "unitCode",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:QuantitativeValue"
                },
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:string"
                },
                {
                    "@type": "@id",
                    "@id": "xsd:string"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:unitCode"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        }
    ]
}