{
    "@context": {
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "geojson": "https://purl.org/geojson/vocab#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "schema": "http://schema.org/",
        "iudx": "https://voc.iudx.org.in/",
        "owl": "http://www.w3.org/2002/07/owl#"
    },
    "@graph": [
        {
            "@id": "iudx:altitude",
            "@type": [
                "iudx:GeoProperty"
            ],
            "rdfs:comment": "Altitude of a place. Specified in WGS 84 CRS (https://en.wikipedia.org/wiki/World_Geodetic_System)",
            "rdfs:label": "altitude",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:GeoCoordinates"
                },
                {
                    "@id": "iudx:Place"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Number"
                },
                {
                    "@id": "iudx:Text"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:elevation"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        }
    ]
}
