{
    "@context": {
        "iudx": "https://iudx.org.in/voc/",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "geojson": "https://purl.org/geojson/vocab#",
        "schema": "http://schema.org/",
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "xsd": "http://www.w3.org/2001/XMLSchema#"
    },
    "@graph": [
        {
            "@id": "iudx:location",
            "@type": [
                "rdf:Property",
                "iudx:GeoProperty"
            ],
            "rdfs:comment": "Location associated with a resource Item (a spatial point), a group of resource items (a spatial region) or even an observation in case of a mobile sensor.",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "location",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Resource"
                },
                {
                    "@id": "iudx:ResourceGroup"
                },
                {
                    "@id": "iudx:Provider"
                },
                {
                    "@id": "iudx:ResourceServer"
                },
                {
                    "@id": "iudx:EnvAQM"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "xsd:String"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:location"
            }
        }
    ]
}
