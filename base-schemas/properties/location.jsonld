{
    "@context": {
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "schema": "http://schema.org/",
        "geojson": "https://purl.org/geojson/vocab#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "iudx": "https://iudx.org.in/voc/",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#"
    },
    "@graph": [
        {
            "@type": "iudx:GeoProperty",
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "xsd:String"
                }
            ],
            "rdfs:comment": "Location associated with a resource (a spatial point), a group of resources (a spatial region) or even an observation in case of a mobile sensor.",
            "@id": "iudx:location",
            "skos:exactMatch": {
                "@id": "schema:location"
            },
            "rdfs:label": "location",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Resource"
                },
                {
                    "@id": "iudx:ResourceGroup"
                },
                {
                    "@id": "iudx:Provider"
                },
                {
                    "@id": "iudx:ResourceServer"
                },
                {
                    "@id": "iudx:EnvAQM"
                }
            ]
        }
    ]
}