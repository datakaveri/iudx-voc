    "@id": "iudx:resourceAuthControlLevel",
    "@type": [
        "rdf:Property",
        "iudx:TextProperty"
    ],
    "rdfs:comment": {
        "@value": "Authorizataion Control Level for resouces associated with a given resource-group. ENUM: [individual, group]. Individual authorization control level indicates that access control is available per resource, i.e., authorization can be granted/not-granted on a per resource basis for the resources. Group authorization control level indicates that the authorization is available on group level only, i.e., authorization will be granted/not-granted for all the resources associated with the given resource group."
    },
    "rdfs:isDefinedBy": {
        "@id": "iudx:"
    },
    "rdfs:label": {
        "@value": "ResourceAuthControlLevel"
    },
