{
    "@context": {
        "iudx": "https://iudx.org.in/voc/",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#"
    },
    "@graph": [
        {
            "@id": "iudx:Thing",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Most generic IUDX object.",
            "rdfs:label": "Thing",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "skos:exactMatch": {
                "@id": "schema:Thing"
            }
        },
        {
            "@id": "iudx:description",
            "@type": [
                "rdf:Property",
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Description for an IUDX thing.",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "description",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Thing"
                },
                {
                    "@id": "iudx:QuantitativeValue"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:string"
                }
            ]
        }
    ]
}