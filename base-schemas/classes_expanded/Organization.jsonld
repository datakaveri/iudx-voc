{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "schema": "http://schema.org/",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://iudx.org.in/voc/",
        "geojson": "https://purl.org/geojson/vocab#",
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "owl": "http://www.w3.org/2002/07/owl#"
    },
    "@graph": [
        {
            "@id": "iudx:Organization",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Information about a given organization (contact info, email, urls etc.)",
            "rdfs:label": "Organization",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "skos:exactMatch": {
                "@id": "schema:Organization"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:Thing"
            }
        },
        {
            "@id": "iudx:address",
            "@type": [
                "iudx:GeoProperty"
            ],
            "rdfs:comment": "Physical address of a place.",
            "rdfs:label": "address",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:Organization"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:address"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:name",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Name of the entity.",
            "rdfs:label": "name",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:Organization"
                },
                {
                    "@id": "iudx:ResourceServer"
                },
                {
                    "@id": "iudx:ResourceGroup"
                },
                {
                    "@id": "iudx:Provider"
                },
                {
                    "@id": "iudx:FunctionDescription"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:name"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        }
    ]
}