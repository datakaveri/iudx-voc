{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "iudx": "https://iudx.org.in/voc/"
    },
    "@graph": [
        {
            "@id": "iudx:Provider",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Describes the Provider entry in the IUDX catalogue.",
            "rdfs:label": "Provider",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:IUDXEntity"
            },
            "iudx:requiredProperties": [
                "name",
                "providerOrg",
                "description"
            ]
        },
        {
            "@id": "iudx:location",
            "@type": [
                "iudx:GeoProperty"
            ],
            "rdfs:comment": "Location associated with a resource Item (a spatial point), a group of resource items (a spatial region) or even an observation in case of a mobile sensor.",
            "rdfs:label": "location",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Resource"
                },
                {
                    "@id": "iudx:ResourceGroup"
                },
                {
                    "@id": "iudx:Provider"
                },
                {
                    "@id": "iudx:ResourceServer"
                },
                {
                    "@id": "iudx:EnvAQM"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:Text"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:location"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:providerOrg",
            "@type": [
                "iudx:StructuredProperty"
            ],
            "rdfs:comment": "Information about the Provider Organization.",
            "rdfs:label": "providerOrg",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Provider"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Organization"
                }
            ]
        },
        {
            "@id": "iudx:name",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Name of the entity.",
            "rdfs:label": "name",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:Organization"
                },
                {
                    "@id": "iudx:ResourceServer"
                },
                {
                    "@id": "iudx:ResourceGroup"
                },
                {
                    "@id": "iudx:Provider"
                },
                {
                    "@id": "iudx:FunctionDescription"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:name"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        }
    ]
}