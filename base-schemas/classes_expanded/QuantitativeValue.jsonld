{
    "@context": {
        "iudx": "https://iudx.org.in/voc/",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#"
    },
    "@graph": [
        {
            "@id": "iudx:QuantitativeValue",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:label": "QuantitativeValue",
            "rdfs:comment": "Describes the value of a Quantitative Property",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:StructuredValue"
            }
        },
        {
            "@id": "iudx:hasFunction",
            "@type": [
                "rdf:Property",
                "iudx:StructuredProperty"
            ],
            "rdfs:comment": "Links a given property/characterstic to a given compute function and specific input parameters.",
            "rdfs:label": "hasFunction",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:QuantitativeValue"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:FunctionDescription"
                }
            ]
        },
        {
            "@id": "iudx:maxValue",
            "@type": [
                "rdf:Property",
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Associated maximum value of a property.",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "maxValue",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:QuantitativeValue"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Number"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:maxValue"
            }
        },
        {
            "@id": "iudx:dataType",
            "@type": [
                "rdf:Property",
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Associated data type. Specified as an URI (linking it to one of the known data type vocabulary, e.g., xsd)",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "dataType",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:QuantitativeValue"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:string",
                    "@type": "@id"
                }
            ]
        },
        {
            "@id": "iudx:unitCode",
            "@type": [
                "rdf:Property",
                "iudx:TextProperty"
            ],
            "rdfs:comment": "The unit of measurement given using the UN/CEFACT Common Code (3 characters) or a URL. Other codes/unit vocabularies may be used with a prefix",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "unitCode",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:QuantitativeValue"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:string"
                },
                {
                    "@id": "xsd:string",
                    "@type": "@id"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:unitCode"
            }
        },
        {
            "@id": "iudx:durationInHours",
            "@type": [
                "rdf:Property",
                "iudx:StructuredProperty"
            ],
            "rdfs:comment": "Specifies the duration of an interval in Hours.",
            "rdfs:label": "durationInHours",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:QuantitativeValue"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:decimal"
                }
            ]
        },
        {
            "@id": "iudx:minValue",
            "@type": [
                "rdf:Property",
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Associated minimum value of a property.",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "minValue",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:QuantitativeValue"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Number"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:minValue"
            }
        },
        {
            "@id": "iudx:unitText",
            "@type": [
                "rdf:Property",
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Describes the unit of measurement. Useful if unitCode is not available.",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "unitText",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:QuantitativeValue"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:string"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:unitText"
            }
        },
        {
            "@id": "iudx:description",
            "@type": [
                "rdf:Property",
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Description for an IUDX thing.",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "description",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Thing"
                },
                {
                    "@id": "iudx:QuantitativeValue"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:string"
                }
            ]
        }
    ]
}