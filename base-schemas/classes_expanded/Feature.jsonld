{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "schema": "http://schema.org/",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "iudx": "https://iudx.org.in/voc/",
        "geojson": "https://purl.org/geojson/vocab#",
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "owl": "http://www.w3.org/2002/07/owl#"
    },
    "@graph": [
        {
            "@id": "iudx:Feature",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Describes the GeoJSON Feature",
            "rdfs:label": "Feature",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "skos:exactMatch": {
                "@id": "geojson:Feature"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:GeoJSONGeometry"
            }
        },
        {
            "@id": "iudx:geometry",
            "@type": [
                "iudx:GeoProperty"
            ],
            "rdfs:comment": {
                "@value": "Geometry property in a GeoJSON object"
            },
            "rdfs:label": "geometry",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Feature"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Point"
                }
            ],
            "skos:exactMatch": {
                "@id": "geojson:geometry"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        }
    ]
}