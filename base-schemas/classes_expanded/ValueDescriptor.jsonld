{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "iudx": "https://iudx.org.in/voc/",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    },
    "@graph": [
        {
            "@id": "iudx:ValueDescriptor",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Value descriptors are JSON objects that describes the value object for a given property.",
            "rdfs:label": "ValueDescriptor",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:Intangible"
            }
        },
        {
            "@id": "iudx:hasFunction",
            "@type": [
                "iudx:StructuredProperty"
            ],
            "rdfs:comment": "Links a given property/characterstic to a given compute function and specific input parameters.",
            "rdfs:label": "hasFunction",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:FunctionDescription"
                }
            ]
        },
        {
            "@id": "iudx:sameAs",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "URL reference to another thing/entity that identifies the given thing/entity. This may be used to provide references to another data models where a property is described or say a reference to Wiki pages for a given thing etc.",
            "rdfs:label": "sameAs",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text",
                    "@type": "@id"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:sameAs"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:maxValue",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Associated maximum value of a property.",
            "rdfs:label": "maxValue",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Number"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:maxValue"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:unitCode",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "The unit of measurement given using the UN/CEFACT Common Code (3 characters) or a URL. Other codes/unit vocabularies may be used with a prefix",
            "rdfs:label": "unitCode",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                },
                {
                    "@id": "iudx:Text",
                    "@type": "@id"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:unitCode"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:durationInHours",
            "@type": [
                "iudx:StructuredProperty"
            ],
            "rdfs:comment": "Specifies the duration of an interval in Hours.",
            "rdfs:label": "durationInHours",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Number"
                }
            ]
        },
        {
            "@id": "iudx:minValue",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Associated minimum value of a property.",
            "rdfs:label": "minValue",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Number"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:minValue"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:unitText",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Describes the unit of measurement. Useful if unitCode is not available.",
            "rdfs:label": "unitText",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:unitText"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:dataType",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Associated data type. Specified as an URI (linking it to one of the known data type vocabulary, e.g., xsd)",
            "rdfs:label": "dataType",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:ValueDescriptor"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text",
                    "@type": "@id"
                }
            ],
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        }
    ]
}