{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "iudx": "https://iudx.org.in/voc/"
    },
    "@graph": [
        {
            "@id": "iudx:FunctionDescription",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Description of a function associated in computation of a quanitative value.",
            "rdfs:label": "FunctionDescription",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:Intangible"
            }
        },
        {
            "@id": "iudx:functionParameters",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "List of parameters associated with a function.",
            "rdfs:label": "functionParameters",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:FunctionDescription"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text",
                    "@container": "@list"
                }
            ]
        },
        {
            "@id": "iudx:name",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Name of the entity.",
            "rdfs:label": "name",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:Organization"
                },
                {
                    "@id": "iudx:ResourceServer"
                },
                {
                    "@id": "iudx:ResourceGroup"
                },
                {
                    "@id": "iudx:Provider"
                },
                {
                    "@id": "iudx:FunctionDescription"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:name"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        }
    ]
}