{
    "@context": {
        "iudx": "https://iudx.org.in/voc/",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "geojson": "https://purl.org/geojson/vocab#",
        "schema": "http://schema.org/",
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#"
    },
    "@graph": [
        {
            "@id": "iudx:GeoCoordinates",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": {
                "@value": "Geo coordinates in Latitude and Longitude for a given place. The Lat/Long values are in WGS84 notation"
            },
            "rdfs:label": "GeoCoordinates",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "skos:closeMatch": {
                "@id": "schema:GeoCoordinates"
            }
        },
        {
            "@id": "iudx:longitude",
            "@type": [
                "rdf:Property",
                "iudx:GeoProperty"
            ],
            "rdfs:comment": "Longitude of a place. Specified in WGS 84 CRS (https://en.wikipedia.org/wiki/World_Geodetic_System)",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "longitude",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:GeoCoordinates"
                },
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:ResourceItem"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:decimal"
                },
                {
                    "@id": "xsd:string"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:longitude"
            }
        },
        {
            "@id": "iudx:latitude",
            "@type": [
                "rdf:Property",
                "iudx:GeoProperty"
            ],
            "rdfs:comment": "Latitude of a place. Specified in WGS 84 CRS (https://en.wikipedia.org/wiki/World_Geodetic_System)",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "latitude",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:GeoCoordinates"
                },
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:ResourceItem"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:decimal"
                },
                {
                    "@id": "xsd:string"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:latitude"
            }
        }
    ]
}