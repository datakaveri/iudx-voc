{
    "@context": {
        "iudx": "https://iudx.org.in/voc/",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "geojson": "https://purl.org/geojson/vocab#",
        "schema": "http://schema.org/",
        "skos": "http://www.w3.org/2004/02/skos/core#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#"
    },
    "@graph": [
        {
            "@id": "iudx:Place",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Describes fixed location for various entities. The places can be described using geo-Coordinates (points, polgons, bboxes etc.) , structured form of addresses or simple address strings.",
            "rdfs:label": "Place",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:StructuredValue"
            },
            "skos:closeMatch": {
                "@id": "schema:Place"
            }
        },
        {
            "@id": "iudx:address",
            "@type": [
                "rdf:Property",
                "iudx:GeoProperty"
            ],
            "rdfs:comment": "Physical address of a place.",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "address",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:Organization"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:string"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:address"
            }
        },
        {
            "@id": "iudx:geoCovers",
            "@type": [
                "rdf:Property",
                "iudx:GeoProperty"
            ],
            "rdfs:comment": "Represents the coverage relationship between two geometries/places.",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "geoCovers",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:ResourceItem"
                },
                {
                    "@id": "iudx:ResourceGroupItem"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:GeoJSONGeometry"
                },
                {
                    "@id": "iudx:Place"
                }
            ]
        },
        {
            "@id": "iudx:longitude",
            "@type": [
                "rdf:Property",
                "iudx:GeoProperty"
            ],
            "rdfs:comment": "Longitude of a place. Specified in WGS 84 CRS (https://en.wikipedia.org/wiki/World_Geodetic_System)",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "longitude",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:GeoCoordinates"
                },
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:ResourceItem"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:decimal"
                },
                {
                    "@id": "xsd:string"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:longitude"
            }
        },
        {
            "@id": "iudx:geo",
            "@type": [
                "rdf:Property",
                "iudx:GeoProperty"
            ],
            "rdfs:comment": {
                "@value": "Geo-coordinates of a place specified in terms of latitude and longitude."
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "geo",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Place"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:GeoCoordinates"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:geo"
            }
        },
        {
            "@id": "iudx:geoJSON",
            "@type": [
                "rdf:Property",
                "iudx:GeoProperty"
            ],
            "rdfs:comment": "Geo-coordinates of a place specified in GeoJSON format.",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "geoJSON",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:ResourceItem"
                },
                {
                    "@id": "iudx:ResourceGroupItem"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:GeoJSONGeometry"
                }
            ]
        },
        {
            "@id": "iudx:latitude",
            "@type": [
                "rdf:Property",
                "iudx:GeoProperty"
            ],
            "rdfs:comment": "Latitude of a place. Specified in WGS 84 CRS (https://en.wikipedia.org/wiki/World_Geodetic_System)",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "latitude",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:GeoCoordinates"
                },
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:ResourceItem"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:decimal"
                },
                {
                    "@id": "xsd:string"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:latitude"
            }
        },
        {
            "@id": "iudx:name",
            "@type": [
                "rdf:Property",
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Name of the entity.",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:label": "name",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:Organization"
                },
                {
                    "@id": "iudx:ResourceServerItem"
                },
                {
                    "@id": "iudx:ResourceGroupItem"
                },
                {
                    "@id": "iudx:ProviderItem"
                },
                {
                    "@id": "iudx:FunctionDescription"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "xsd:decimal"
                },
                {
                    "@id": "xsd:string"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:name"
            }
        }
    ]
}