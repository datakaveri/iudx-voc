{
    "@context": {
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "iudx": "https://iudx.org.in/voc/"
    },
    "@graph": [
        {
            "@id": "iudx:ResourceServer",
            "@type": [
                "owl:Class",
                "rdfs:Class"
            ],
            "rdfs:comment": "Describes the Resource Server entry in the IUDX catalogue.",
            "rdfs:label": "ResourceServer",
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            },
            "rdfs:subClassOf": {
                "@id": "iudx:IUDXEntity"
            },
            "iudx:requiredProperties": [
                "name",
                "resourceServerOrg",
                "resourceServerHTTPAccessURL",
                "description"
            ]
        },
        {
            "@id": "iudx:resourceServerOrg",
            "@type": [
                "iudx:StructuredProperty"
            ],
            "rdfs:comment": "Information about the organization operating the Resource Server.",
            "rdfs:label": "resourceServerOrg",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:ResourceServer"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Organization"
                }
            ]
        },
        {
            "@id": "iudx:geoCovers",
            "@type": [
                "iudx:GeoProperty"
            ],
            "rdfs:comment": "Represents the coverage relationship between two geometries/places.",
            "rdfs:label": "geoCovers",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:Resource"
                },
                {
                    "@id": "iudx:ResourceGroup"
                },
                {
                    "@id": "iudx:ResourceServer"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:GeoJSONGeometry"
                },
                {
                    "@id": "iudx:Place"
                }
            ],
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:location",
            "@type": [
                "iudx:GeoProperty"
            ],
            "rdfs:comment": "Location associated with a resource Item (a spatial point), a group of resource items (a spatial region) or even an observation in case of a mobile sensor.",
            "rdfs:label": "location",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Resource"
                },
                {
                    "@id": "iudx:ResourceGroup"
                },
                {
                    "@id": "iudx:Provider"
                },
                {
                    "@id": "iudx:ResourceServer"
                },
                {
                    "@id": "iudx:EnvAQM"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:Text"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:location"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        },
        {
            "@id": "iudx:resourceServerHTTPAccessURL",
            "@type": [
                "iudx:Relationship"
            ],
            "rdfs:comment": "Resource Server URL for data access based on HTTP/REST APIs",
            "rdfs:label": "resourceServerHTTPAccessURL",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:ResourceServer"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text",
                    "@type": "@id"
                },
                {
                    "@id": "iudx:RelationshipValue"
                }
            ]
        },
        {
            "@id": "iudx:name",
            "@type": [
                "iudx:TextProperty"
            ],
            "rdfs:comment": "Name of the entity.",
            "rdfs:label": "name",
            "iudx:domainIncludes": [
                {
                    "@id": "iudx:Place"
                },
                {
                    "@id": "iudx:Organization"
                },
                {
                    "@id": "iudx:ResourceServer"
                },
                {
                    "@id": "iudx:ResourceGroup"
                },
                {
                    "@id": "iudx:Provider"
                },
                {
                    "@id": "iudx:FunctionDescription"
                }
            ],
            "iudx:rangeIncludes": [
                {
                    "@id": "iudx:Text"
                }
            ],
            "skos:exactMatch": {
                "@id": "schema:name"
            },
            "rdfs:isDefinedBy": {
                "@id": "iudx:"
            }
        }
    ]
}