{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "<iudx>/Resource",
    "title": "Resource",
    "describes": "Describes the Resource entry in the IUDX catalogue.",
    "type": "object",
    "required": [
        "tags",
        "resourceGroup",
        "description"
    ],
    "properties": {
        "geoJSON": {
            "$ref": "<iudx>/geoJSON"
        },
        "resourceGroup": {
            "$ref": "<iudx>/resourceGroup"
        },
        "location": {
            "$ref": "<iudx>/location"
        },
        "geoCovers": {
            "$ref": "<iudx>/geoCovers"
        },
        "resourceServer": {
            "$ref": "<iudx>/resourceServer"
        },
        "dataModel": {
            "$ref": "<iudx>/dataModel"
        },
        "hasResourceGroup": {
            "$ref": "<iudx>/hasResourceGroup"
        },
        "hasResourceServer": {
            "$ref": "<iudx>/hasResourceServer"
        },
        "provider": {
            "$ref": "<iudx>/provider"
        },
        "tags": {
            "$ref": "<iudx>/tags"
        },
        "hasProvider": {
            "$ref": "<iudx>/hasProvider"
        }
    }
}