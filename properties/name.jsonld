{
    "@context": {
        "iudx": "https://iudx.org.in/voc/",
        "owl": "http://www.w3.org/2002/07/owl#",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#"
    },
    "@id": "iudx:name",
    "@type": [
        "rdf:Property",
        "iudx:Property"
    ],
    "owl:equivalentProperty": {
        "@id": "dcterms:title"
    },
    "rdfs:comment": "The name of the property",
    "rdfs:label": "name",
    "rdfs:subPropertyOf": {
        "@id": "rdfs:label"
    },
    "iudx:domainIncludes": {
        "@id": "iudx:PropertyValue"
    },
    "iudx:rangeIncludes": {
        "@id": "iudx:Text"
    }
}
